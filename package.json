{
  "name": "gta6-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:seo && vite build",
    "prebuild": "node ./scripts/generate-sitemap.mjs && node ./scripts/generate-rss.mjs",
    "preview": "vite preview",
    "lint": "eslint .",
    "sitemap": "SITE_URL=https://gta6-site.vercel.app node ./scripts/generate-sitemap.mjs",
    "rss": "SITE_URL=https://gta6-site.vercel.app node ./scripts/generate-rss.mjs",
    "build:seo": "npm run sitemap && npm run rss"
  },
  "dependencies": {
    "dompurify": "^3.2.6",
    "framer-motion": "^12.23.12",
    "marked": "^16.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2"
  }
}
